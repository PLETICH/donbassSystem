let xpAdd = Math.floor(Math.random() * 1) + 100;
    console.log(xpAdd);
  
    if(!xp[message.author.id]){
      xp[message.author.id] = {
        xp: 0
      };
    }
  
  let curxp = xp[message.author.id].xp;
  xp[message.author.id].xp = curxp + xpAdd;

  fs.writeFile('./xp.json',JSON.stringify(xp),(err) => {
    if(err) console.log(err);
    const logChannel = message.guild.channels.find(channel => channel.id === '900054877919658064') 
    let xpEmbed = new Discord.RichEmbed()
    .setTitle(`–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å ${message.author.tag}`)
    .setDescription(`<@${message.author.id}> —Ç–æ–ª—å–∫–æ —á—Ç–æ –ø–æ–ª—É—á–∏–ª 100 –æ—á–∫–æ–≤ –∑–∞ —Å–æ–æ–±—â–µ–Ω–∏–µ =^)`)
    .addField(`üç¨ –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –≤—Å–µ–≥–æ –æ—á–∫–æ–≤:`, `${xp[message.author.id].xp}`)
    .setColor("#ffce2d")
    .setFooter(`ID: ${message.author.id} ‚Ä¢ ${message.createdAt}`)
    .setThumbnail(message.author.displayAvatarURL)
    logChannel.send(xpEmbed)
});
